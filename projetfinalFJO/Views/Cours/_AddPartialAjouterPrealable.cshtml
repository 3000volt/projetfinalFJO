@model projetfinalFJO.Appdata.Prealables
<h4>Prealables</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AssocierPrealable" asp-controller="Cours">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                <div class="form-group">
                    <label asp-for="NoCoursPrealable" class="control-label"></label>
                    <select asp-for="NoCoursPrealable" class="form-control" asp-items="ViewBag.CoursList"></select>
                </div>
            <div class="form-group">
                <input type="submit" value="Ajouter" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
