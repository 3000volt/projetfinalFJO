@model projetfinalFJO.Appdata.Famillecompetence

<h4>Associer une famille</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NomFamille" class="control-label"></label>
                <select asp-for="NomFamille" class="form-control" asp-items="ViewBag.NomFamille"></select>
            </div>
            <div class="form-group">
                <input type="button" value="Create" onclick="AssocierFamilleAjax()" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
