@*@{
        ViewData["Title"] = "CreerAnalyse";
    }*@
<div id="principale">
    <h2>Création d'analyse</h2>

</div>

<div id="divCompetence">
    <partial name="_AnalyseCompetence" />@*model="ViewBag.CreerAnalyse"*@
</div>
<hr />

<div id="tabs">
    <ul>
        <li class="loic"><a href="@Url.Action("PartialListeFamille", "AnalyseCompetence")">Assigner une famille</a></li>
        <li><a href="@Url.Action("PartialAjouterFamille", "AnalyseCompetence")">Créer une famille </a></li>
    </ul>
</div>
<hr />

<div id="tabs2">
    <ul>
        <li class="loic"><a href="@Url.Action("PartialListeSequence", "AnalyseCompetence")">Assigner une séquence</a></li>
        <li><a href="@Url.Action("PartialAjouterSequence", "AnalyseCompetence")">Créer une séquence </a></li>
    </ul>
</div>
<hr />

<div id="divElementCompetence">
    <partial name="_AnalyseElementCompetence" />@*model="ViewBag.CreerAnalyse"*@
</div>

@section Scripts
    {
    <script src="~/js/Analyse.js"></script>
    <script>
        /*https://jqueryui.com/tabs/#ajax  */
        $(function () {
            $("#tabs").tabs({
                beforeLoad: function (event, ui) {
                    ui.jqXHR.fail(function () {
                        ui.panel.html(
                            "Couldn't load this tab. We'll try to fix this as soon as possible. " +
                            "If this wouldn't be a demo.");
                    });
                }
            });
        });

    </script>
    <script>
        /*https://jqueryui.com/tabs/#ajax  */
        $(function () {
            $("#tabs2").tabs({
                beforeLoad: function (event, ui) {
                    ui.jqXHR.fail(function () {
                        ui.panel.html(
                            "Couldn't load this tab. We'll try to fix this as soon as possible. " +
                            "If this wouldn't be a demo.");
                    });
                }
            });
        });

    </script>
}