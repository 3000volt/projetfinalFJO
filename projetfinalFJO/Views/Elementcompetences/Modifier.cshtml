@model projetfinalFJO.Appdata.Elementcompetence
@inject Microsoft.AspNetCore.Http.IHttpContextAccessor HttpContextAccessor
@{
    ViewData["Title"] = "Modifier";
}

<h2>Modifier</h2>

<h4>Elementcompetence</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Modifier">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ElementCompétence" class="control-label"></label>
                <input asp-for="ElementCompétence" class="form-control" />
                <span asp-validation-for="ElementCompétence" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CriterePerformance" class="control-label"></label>
                <input asp-for="CriterePerformance" class="form-control" />
                <span asp-validation-for="CriterePerformance" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="NoProgramme" class="control-label"></label>
                <input asp-for="NoProgramme" class="form-control" />
                <span asp-validation-for="NoProgramme" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Enregistrer" class="btn btn-info" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="ListeElementCompetence2" class="btn btn-info" style="background-color:darkblue;border-color:darkblue">Retour à la liste</a>
</div>

