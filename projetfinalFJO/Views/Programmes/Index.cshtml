@model IEnumerable<projetfinalFJO.Appdata.Programmes>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomProgramme)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NbHeure)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NbUnite)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NbCompetencesObligatoires)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NbCompetencesOptionnelles)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CondtionsAdmission)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomProgramme)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NbHeure)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NbUnite)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NbCompetencesObligatoires)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NbCompetencesOptionnelles)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CondtionsAdmission)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.NoProgramme">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.NoProgramme">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.NoProgramme">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
</body>
</html>
