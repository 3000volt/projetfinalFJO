@model projetfinalFJO.Appdata.GroupeCompetence

<h4>GroupeCompetence</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="_PartialGroupeComp">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NomGroupe" class="control-label"></label>
                <select asp-for="NomGroupe" class="form-control" asp-items="ViewBag.NomGroupe"></select>
            </div>
            <div class="form-group">
                <label asp-for="NomSession" class="control-label"></label>
                <select asp-for="NomSession" class="form-control" asp-items="ViewBag.NomSession"></select>
            </div>
            <div class="form-group">
                <input type="button" value="Create" class="btn btn-default" onclick="fnGroupeCompAjax()" />
            </div>
        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
