@model projetfinalFJO.Appdata.Elementcompetence

@{
    ViewData["Title"] = "_PartialElementcompetence";
}



<h4>Créer des élements de competence</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="ElementCompétence" class="control-label"></label>
                <input asp-for="ElementCompétence" class="form-control" />
                <span asp-validation-for="ElementCompétence" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="CriterePerformance" class="control-label"></label>
                <textarea asp-for="CriterePerformance" class="form-control"></textarea>
                <span asp-validation-for="CriterePerformance" class="text-danger"></span>
            </div>            
            <div class="form-group">
                <input type="button" value="créer l'élément" class="btn btn-info" onclick="fnAddelecommpetenceAjax()" />
            </div>
        </form>
    </div>
</div>


