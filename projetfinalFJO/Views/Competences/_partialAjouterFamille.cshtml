@model projetfinalFJO.Appdata.Famillecompetence

<h4>Ajouter une famille</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form>
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="NomFamille" class="control-label"></label>
                <input id="NomFamille2" class="form-control" />
            </div>
            <div class="form-group">
                <input type="button" value="Create" class="btn btn-default" onclick="AjouterFamilleAjax()" />
            </div>
        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
