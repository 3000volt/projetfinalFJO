@model projetfinalFJO.Appdata.Competences

@{
    ViewData["Title"] = "Delete";
}
<h3>Voulez-vous vraiment supprimer cette compétence?</h3>
<div>
    <h4>Competences</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.ObligatoireCégep)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ObligatoireCégep)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Description)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContextRealisation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContextRealisation)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.NoProgrammeNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NoProgrammeNavigation.NoProgramme)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.NomFamilleNavigation)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.NomFamilleNavigation.NomFamille)
        </dd>
    </dl>
    
    <form asp-action="Delete" asp-route-id="@Model.CodeCompetence">
        <input type="hidden" asp-for="CodeCompetence" />
        <input type="submit" value="Supprimer" class="btn btn-info"  /> |
        <a asp-action="ListeCompetence">Retour à la liste</a>
    </form>
</div>
